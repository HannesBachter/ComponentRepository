	t1:requiredService t3:SmartGMapping_LaserServiceIn;
	# ComponentPort LocalizationUpdateServiceOut
	t1:requiredService t3:SmartGMapping_LocalizationUpdateServiceOut;
	# ComponentPort CurrGridMapPushServiceOut
	t1:providedService t3:SmartGMapping_CurrGridMapPushServiceOut;
